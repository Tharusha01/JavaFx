<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #4267b2;" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaFx.TransactionController">

    <children>

        <Button layoutX="630.0" layoutY="530.0" mnemonicParsing="false" onMouseClicked="#btnBackToHome" style="-fx-background-color: white;" text="LogOut">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
        </Button>
      <Button layoutX="370.0" layoutY="487.0" mnemonicParsing="false" onMouseClicked="#btnLoadTransDetails" style="-fx-background-color: white;" text="Load Details">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TableView fx:id="tableTransactions" layoutX="3.0" layoutY="115.0" prefHeight="354.0" prefWidth="793.0">
         <columns>
            <TableColumn fx:id="tbTCDate" prefWidth="89.0" text="Date" />
            <TableColumn fx:id="tbTCEmail" prefWidth="140.0" text="Customer Email" />
            <TableColumn fx:id="tbTProductsDetails" prefWidth="511.0" text="Products Details" />
            <TableColumn fx:id="tbPTotPrice" prefWidth="51.0" text="Total" />
         </columns>
      </TableView>
      <Label layoutX="235.0" layoutY="34.0" text="Transactions Details" textFill="WHITE">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
    </children>

</AnchorPane>
