<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #4267b2;"
            xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaFx.userController">
   <children>
      <Button layoutX="640.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#btnBackToHome" style="-fx-background-color: white;" text="Back To Home">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="267.0" layoutY="46.0" text="Registerd Users" textFill="WHITE">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <TableView fx:id="tableCustomer" layoutX="4.0" layoutY="118.0" prefHeight="340.0" prefWidth="793.0">
         <columns>
            <TableColumn fx:id="tbCusName" prefWidth="217.0" text="Customer Name" />
            <TableColumn fx:id="tbCusEmail" prefWidth="271.0" text="Customer Email" />
            <TableColumn fx:id="tbCusAddress" prefWidth="302.0" text="Customer Address" />
         </columns>
      </TableView>
      <Button layoutX="360.0" layoutY="476.0" mnemonicParsing="false" onMouseClicked="#btnLoadStaffDetails" style="-fx-background-color: white;" text="Load Details">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="lbPError" layoutX="298.0" layoutY="518.0" prefHeight="31.0" prefWidth="293.0" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
